{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.bullet": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        {
                            "move": "q.is_alive"
                        }
                    ]
                },
                "move": {
                    "animations": [
                        "move"
                    ],
                    "transitions": [
                        {
                            "default": "query.mark_variant == 2"
                        }
                    ]
                }
            }
        }
    }
}