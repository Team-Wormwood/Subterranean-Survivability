{
    "format_version": "1.16.100",
    "minecraft:block": {
        "description": {
            "identifier": "yana:mechanical_crafting_table",
            "properties": {
                "yana:texture_frame": [
                    1,
                    2
                ]
            }
        },
        "components": {
            "minecraft:material_instances": {
                "*": {
                    "render_method": "alpha_test",
                    "texture": "mechanical_crafting_table",
                    "ambient_occlusion": true,
                    "face_dimming": true
                }
            },
            "minecraft:ticking": {
                "looping": true,
                "range": [
                    0.25,
                    0.25
                ],
                "on_tick": {
                    "event": "frame_update"
                }
            },
            "minecraft:block_light_absorption": 0,
            "minecraft:block_light_emission": 0.0,
            "minecraft:explosion_resistance": 5,
            "minecraft:destroy_time": 3,
            "minecraft:crafting_table": {
                "custom_description": "Mechanical Crafting Table",
                "grid_size": 3,
                "crafting_tags": [
                    "mechanical_crafting_table"
                ]
            },
            "tag:wood": {}
        },
        "events": {
            "frame_update": {
                "sequence": [
                    {
                        "condition": "q.block_property('yana:texture_frame') == 1",
                        "set_block_property": {
                            "yana:texture_frame": 2
                        }
                    },
                    {
                        "condition": "q.block_property('yana:texture_frame') == 2",
                        "set_block_property": {
                            "yana:texture_frame": 1
                        }
                    }
                ]
            }
        },
        "permutations": [
            {
                "condition": "q.block_property('yana:texture_frame') == 1",
                "components": {
                    "minecraft:geometry": "geometry.mechanical_table_0"
                }
            },
            {
                "condition": "q.block_property('yana:texture_frame') == 2",
                "components": {
                    "minecraft:geometry": "geometry.mechanical_table_1"
                }
            }
        ]
    }
}