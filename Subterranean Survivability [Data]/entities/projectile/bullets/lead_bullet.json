{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "yanasakana:lead_bullet_entity",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false,
            "runtime_identifier": "arrow"
        },
        "component_groups": {
            "fuseParticle": {
                "minecraft:timer": {
                    "looping": true,
                    "time": 0.1,
                    "time_down_event": {
                        "event": "fuseParticle"
                    }
                }
            },
            "fuseParticle2": {
                "minecraft:timer": {
                    "looping": true,
                    "time": 0.2,
                    "time_down_event": {
                        "event": "fuseParticle2"
                    }
                }
            }
        },
        "components": {
            "minecraft:projectile": {
                "on_hit": {
                    "impact_damage": {
                        "damage": 10,
                        "knockback": false,
                        "destroy_on_hit": true
                    },
                    "remove_on_hit": {},
                    "definition_event": {
                        "affect_projectile": false,
                        //"affect_target": true,
                        "event_trigger": {
                            "event": "bulletLand"
                        }
                    }
                },
                "mob_effect": {
                    "effect": "poison",
                    "durationeasy": 5,
                    "durationnormal": 5,
                    "durationhard": 5,
                    "amplifier": 2
                },
                "reflect_on_hurt": true,
                "should_bounce": true,
                "gravity": 0.05,
                "inertia": 0.99,
                "power": 6,
                "potion_effect": 25,
                "catch_fire": false,
                "shoot_target": true,
                "is_dangerous": true
            },
            "minecraft:physics": {
                "has_collision": true
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "fuseParticle"
                    ]
                }
            },
            "bulletLand": {
                "sequence": [
                    {
                        "run_command": {
                            "command": [
                                "playsound block.bamboo_sapling.place @a[r=4] ~ ~ ~ 5 1.5 0.5",
                                "particle minecraft:basic_smoke_particle ~ ~ ~"
                            ]
                        }
                    },
                    {
                        "run_command": {
                            "command": [
                                "effect @s poison 3 4"
                            ],
                            "target": "other"
                        }
                    }
                ]
            },
            "fuseParticle": {
                "run_command": {
                    "command": [
                        "particle yanasakana:dynamite_fuse_smoke ~ ~ ~",
                        "particle yanasakana:dynamite_fuse_flame ~ ~ ~"
                    ]
                }
            },
            "fuseParticle2": {
                "run_command": {
                    "command": [
                        "particle yanasakana:dynamite_fuse_smoke ~ ~ ~",
                        "particle yanasakana:dynamite_fuse_flame ~ ~ ~"
                    ]
                }
            }
        }
    }
}